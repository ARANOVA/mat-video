    "pkg:linux": "cd dist/mat-video && npm pack",
    "test": "npx ng test mat-video --code-coverage",
    "lint": "npx eslint \"projects/**/*.ts\" --fix",
    "update-angular": "npx npm-check-updates --upgrade --target \"minor\" --filter \"/@angular.*/\"",
